@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<form method="post">
      

    <h4>Kyle's JMD TO USD Converter</h4>
  
    <label>From <br />JMD : </label>
    <input type="number" asp-for="JMD"/>

    <label>To <br />USD : </label>
    <input type="number" asp-for="USD" id="value"/>

    <input type="submit"/>

    <p> @ViewData["Convert"]</p>

</form>